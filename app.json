{
  "expo": {
    "name": "Dengo",
    "slug": "dengo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Dengo_logo_png.png",
    "scheme": "dengo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Dengo_logo_png.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Dengo necesita acceso a tu ubicaci칩n para mostrarte las sucursales m치s cercanas y calcular distancias."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA"
      ],
      "package": "com.luisgosi.dengo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Dengo necesita acceso a tu ubicaci칩n para mostrarte las sucursales m치s cercanas y calcular distancias."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f97890f1-560a-4c45-83cd-a41300a1ad6c"
      }
    },
    "owner": "luisgosi"
  }
}
